#!/bin/false
# This files purpose is to be included, not to be called

if [ -d ~/.bash_alias.d/ ] && [ "$BASH_ALIAS_D_INCLUDED" != "1" ]
then
    for file in $(ls -1 ~/.bash_alias.d/)
    do
        . ~/.bash_alias.d/${file}
    done

    # Protect functions from being included more than one time
    BASH_ALIAS_D_INCLUDED=1
fi