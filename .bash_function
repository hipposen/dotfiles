#!/bin/false
# This files purpose is to be included, not to be called

if [ -d ~/.bash_function.d/ ] && [ "$BASH_FUNCTION_D_INCLUDED" != "1" ]
then
    for file in $(ls -1 ~/.bash_function.d/)
    do
        . ~/.bash_function.d/${file}
    done

    # Protect functions from being included more than one time
    BASH_FUNCTION_D_INCLUDED=1
fi